{
  "extends": "./tsconfig-shared.json",
  "compilerOptions": {
    "outDir": "dist/web",
    "typeRoots": ["node_modules/@types", "types"]
  },
  "files": [
    "web/main.ts",
    "web/polyfills.ts"
